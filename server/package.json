{
  "name": "claude-prompts-server",
  "version": "1.0.0",
  "description": "Custom Prompts API Server for OpenAI integration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsc -w & node --watch dist/server.js",
    "test": "node test-server.js",
    "setup": "node scripts/setup-s3.js",
    "migrate": "node scripts/migrate-to-s3.js",
    "logo": "node scripts/convert-logo.js",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.400.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.0.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "canvas": "^2.11.2",
    "sharp": "^0.32.6",
    "typescript": "^5.3.3"
  }
}
